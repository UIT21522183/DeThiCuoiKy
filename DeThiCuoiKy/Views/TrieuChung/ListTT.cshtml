@model List<object>
@{
    ViewData["Title"] = "Liệt kê CN";
}

@{
    <h2>Liệt kê CN</h2>
   <form action="">
        <table border="1">
            <tr>
                <th>Tên CN</th>
                <th>Nam sinh</th>
                <th>Noi ve</th>
                <th>So trieu chung</th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td> @item.GetType().GetProperty("tencn").GetValue(item,null)</td>
                    <td>@item.GetType().GetProperty("namsinh").GetValue(item,null)</td>
                    <td>@item.GetType().GetProperty("nuocve").GetValue(item,null)</td>
                    <td>@item.GetType().GetProperty("sl").GetValue(item,null)</td>
                </tr>
            }
        </table>
   </form>
}
