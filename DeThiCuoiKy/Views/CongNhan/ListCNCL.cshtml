 @model List<CONGNHAN>

@{
    <form method="post" action="">
     <table>
     <tr>
     <th>Mã công nhân</th>
     <th>Tên công nhân</th>
     <th>Chức năng</th>
     </tr>
     @foreach(var item in ViewBag.congnhan )
                {
                <tr>
                    <td>@item.MaCongNhan</td>
                    <td>@item.TenCongNhan</td>
                    <td>
                        <a href="ViewCN?Id=@item.MaCongNhan">View</a>
                        <a href="DeleteCN?Id=@item.MaCongNhan">Delete</a>
                    </td>
                </tr>
                }
   
 
     </table>
    </form>
}
